{
	"manifest_version": 3,
	"name": "Collection",
	"author": "mienaiyami",
	"homepage_url": "https://github.com/mienaiyami",
	"version": "0.0.1",
	"description": "Chromium Extension to keep URLs in a ordered way.",
	"action": {
		"default_popup": "index.html"
	},
	"permissions": [
		"tabs",
		"activeTab",
		"storage",
		"scripting",
		"webRequest"
	],
	"host_permissions": [
		"<all_urls>"
	]
}